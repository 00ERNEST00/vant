    "pub": "sh build/release.sh",
    "pub:all": "node build/bin/build-all.js && lerna publish",
    "clean": "rimraf lib && rimraf packages/*/lib"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.qima-inc.com:fe/oxygen.git"
  },
