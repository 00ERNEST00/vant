{
  "name": "oxygen",
  "version": "0.0.1",
  "description": "有赞vue wap组件库",
  "main": "lib/oxygen.common.js",
  "style": "lib/style.css",
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "scripts": {
    "dev": "npm run bootstrap && npm run build:entry",
    "bootstrap": "npm i",
    "dist": "npm run clean && npm run build:entry",
